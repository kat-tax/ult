{
  "name": "AppTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "web": "ult-scripts run-web",
    "ios": "react-native run-ios",
    "macos": "node node_modules/react-native-macos/local-cli/cli.js start --use-react-native-macos",
    "windows": "react-native run-windows",
    "android": "react-native run-android",
    "build-web": "ult-scripts build-web",
    "build-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/app/src/main/assets/index.ios.bundle --assets-dest ios/app/src/main/res",
    "build-macos": "react-native bundle --platform macos --dev false --entry-file index.js --bundle-output macos/app/src/main/assets/index.macos.bundle --assets-dest macos/app/src/main/res",
    "build-windows": "react-native bundle --platform windows --dev false --entry-file index.js --bundle-output windows/app/src/main/assets/index.windows.bundle --assets-dest windows/app/src/main/res",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "ult-scripts build-web",
    "start": "ult-scripts run-web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest"
  },
  "dependencies": {
    "@lingui/react": "^3.8.9",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-native": "0.63.4",
    "react-native-macos": "0.63.23",
    "react-native-windows": "0.63.24",
    "react-native-web": "^0.15.6",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "1.13.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-vector-icons": "^8.1.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.15",
    "@react-navigation/devtools": "^5.1.22",
    "@lingui/cli": "^3.8.9",
    "@lingui/macro": "^3.8.9",
    "@types/node": "^14.14.37",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-native-vector-icons": "^6.4.6",
    "eslint": "^7.24.0",
    "metro-react-native-babel-preset": "^0.65.2",
    "typescript": "^4.2.4",
    "ult-scripts": "^1.0.48"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
